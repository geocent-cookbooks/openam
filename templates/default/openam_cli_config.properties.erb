BASE_DIR= <%= node["tomcat-openam"]["cfg"]["base-dir"] %>
locale=<%= node["tomcat-openam"]["cfg"]["locale"] %>
PLATFORM_LOCALE=<%= node["tomcat-openam"]["cfg"]["platform-locale"] %>
AM_ENC_KEY=
ADMIN_PWD=<%= node["tomcat-openam"]["cfg"]["admin-pwd"] %>
AMLDAPUSERPASSWD=<%= node["tomcat-openam"]["cfg"]["amldapuserpasswd"] %>
COOKIE_DOMAIN=<%= node["tomcat-openam"]["cfg"]["cookie-domain"] %>
